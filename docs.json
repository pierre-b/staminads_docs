{
  "$schema": "https://mintlify.com/docs.json",
  "theme": "mint",
  "name": "Staminads Documentation",
  "colors": {
    "primary": "#7763F1",
    "light": "#8B63F1"
  },
  "favicon": "/favicon.svg",
  "appearance": {
    "default": "light",
    "strict": true
  },
  "navigation": {
    "tabs": [
      {
        "tab": "Documentation",
        "groups": [
          {
            "group": "Get Started",
            "pages": [
              "introduction",
              "installation",
              "web-sdk",
              "traffic-categorization",
              "metrics-dimensions",
              "dashboard",
              "goals",
              "explore-report",
              "annotations",
              "users-api-keys"
            ]
          }
        ]
      },
      {
        "tab": "API Reference",
        "groups": [
          {
            "group": "Workspaces",
            "openapi": "openapi.json",
            "pages": [
              "GET /api/workspaces.list",
              "GET /api/workspaces.get",
              "POST /api/workspaces.create",
              "POST /api/workspaces.update",
              "POST /api/workspaces.delete"
            ]
          },
          {
            "group": "Analytics",
            "openapi": "openapi.json",
            "pages": [
              "POST /api/analytics.query",
              "POST /api/analytics.extremes",
              "GET /api/analytics.metrics",
              "GET /api/analytics.dimensions",
              "GET /api/analytics.tables"
            ]
          },
          {
            "group": "Filters",
            "openapi": "openapi.json",
            "pages": [
              "GET /api/filters.list",
              "GET /api/filters.get",
              "POST /api/filters.create",
              "POST /api/filters.update",
              "POST /api/filters.delete",
              "POST /api/filters.reorder",
              "GET /api/filters.listTags",
              "POST /api/filters.backfillStart",
              "GET /api/filters.backfillStatus",
              "POST /api/filters.backfillCancel",
              "GET /api/filters.backfillList",
              "GET /api/filters.backfillSummary"
            ]
          },
          {
            "group": "Export",
            "openapi": "openapi.json",
            "pages": [
              "GET /api/export.userEvents"
            ]
          }
        ]
      }
    ]
  },
  "logo": {
    "light": "/logo/logo.svg",
